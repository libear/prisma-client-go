datasource db {
	provider = "postgresql"
	url      = env("__REPLACE__")
}

generator db {
	provider = "go run github.com/prisma/prisma-client-go"
	output = "."
	disableGoBinaries = true
	package = "db"
  previewFeatures = ["mongoDb"]
}

model User {
	id       String  @id @default(cuid()) @map("_id")
	email    String
	name     String?
}
